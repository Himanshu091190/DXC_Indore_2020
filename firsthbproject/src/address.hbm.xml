<hibernate-mapping>
	<class name="com.dxc.hb.Address" table="address">
		<id name="addressid">
			<!-- <generator class="foreign"> <param name="property">employee</param> 
				</generator> -->
			<generator vlass="increment"></generator>
		</id>

